    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x", "+", 
           RowBox[{
            FractionBox["1", "2"], " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"-", "\[Theta]x1"}], "-", "\[Theta]x2"}], ")"}]}]}], 
          ")"}], " ", 
         RowBox[{"(", 
          RowBox[{
