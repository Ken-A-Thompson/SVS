Cell["\<\
area of overlap as function of distance from optimum (d) and angle between \
optima (\[Theta])\
\>", "Text",
 CellChangeTimes->{{3.725225521764359*^9, 3.7252255437628603`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"A", "[", 
   RowBox[{"d_", ",", "\[Theta]_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"2", 
     SuperscriptBox["d", "2"], " ", 
     RowBox[{"ArcCos", "[", 
      FractionBox["x", 
       RowBox[{"2", "d"}]], "]"}]}], "-", 
    RowBox[{
     FractionBox["x", "2"], "  ", 
     SqrtBox[
      RowBox[{
       RowBox[{"4", " ", 
        SuperscriptBox["d", "2"]}], "-", 
       SuperscriptBox["x", "2"]}]]}]}], "/.", 
   RowBox[{"x", "->", 
    RowBox[{"d", 
     SqrtBox[
      RowBox[{"2", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ")"}]}]]}]}]}]}]], "Input",
