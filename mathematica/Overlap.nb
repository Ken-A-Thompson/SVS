Cell["volume overlap m=3", "Section",
 CellChangeTimes->{{3.726871335689309*^9, 3.7268713406733103`*^9}}],

Cell["\<\
http://mathworld.wolfram.com/Sphere-SphereIntersection.html\
\>", "Text",
 CellChangeTimes->{3.726871357809917*^9}],

Cell["\<\
volume overlap bw spheres of radii r and R, with distance d between the \
centres\
\>", "Text",
 CellChangeTimes->{{3.726871329729908*^9, 3.7268713315130453`*^9}, {
  3.726871362696957*^9, 3.726871393962339*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 FractionBox[
  RowBox[{"\[Pi]", " ", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"R", "+", "r", "-", "d"}], ")"}], "2"], 
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["d", "2"], "+", 
     RowBox[{"2", "d", " ", "r"}], "-", 
     RowBox[{"3", 
      SuperscriptBox["r", "2"]}], "+", 
     RowBox[{"2", "d", " ", "R"}], "+", 
     RowBox[{"6", "r", " ", "R"}], "-", 
     RowBox[{"3", 
      SuperscriptBox["R", "2"]}]}], ")"}]}], 
  RowBox[{"12", "d"}]]], "Input",
 CellChangeTimes->{{3.726871395613381*^9, 3.726871396237629*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{"\[Pi]", " ", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "d"}], "+", "r", "+", "R"}], ")"}], "2"], " ", 
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["d", "2"], "+", 
     RowBox[{"2", " ", "d", " ", "r"}], "-", 
     RowBox[{"3", " ", 
      SuperscriptBox["r", "2"]}], "+", 
     RowBox[{"2", " ", "d", " ", "R"}], "+", 
     RowBox[{"6", " ", "r", " ", "R"}], "-", 
     RowBox[{"3", " ", 
      SuperscriptBox["R", "2"]}]}], ")"}]}], 
  RowBox[{"12", " ", "d"}]]], "Output",
 CellChangeTimes->{
  3.726871396677092*^9, {3.726871520740383*^9, 3.7268715485196447`*^9}, 
   3.7268717286944017`*^9}]
}, Open  ]],

Cell["when both have same radii", "Text",
 CellChangeTimes->{{3.7268714074668694`*^9, 3.7268714124105577`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"%", "/.", 
   RowBox[{"R", "\[Rule]", "r"}]}], "//", "Simplify"}]], "Input",
 CellChangeTimes->{{3.72687139829277*^9, 3.726871404987874*^9}, {
  3.7268717210221043`*^9, 3.726871725041589*^9}}],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "12"], " ", "\[Pi]", " ", 
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{"d", "-", 
     RowBox[{"2", " ", "r"}]}], ")"}], "2"], " ", 
  RowBox[{"(", 
   RowBox[{"d", "+", 
    RowBox[{"4", " ", "r"}]}], ")"}]}]], "Output",
 CellChangeTimes->{
  3.7268714140980787`*^9, {3.72687152085562*^9, 3.726871548619882*^9}, 
   3.726871730278471*^9}]
}, Open  ]],

Cell["\<\
and when the distance between them is determined by the distance from the \
origin (also r) and the angle between them\
\>", "Text",
 CellChangeTimes->{{3.726871419898899*^9, 3.726871458484112*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"V", "[", 
   RowBox[{"r_", ",", "\[Theta]_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox["1", "12"], " ", "\[Pi]", " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"d", "-", 
       RowBox[{"2", " ", "r"}]}], ")"}], "2"], " ", 
    RowBox[{"(", 
     RowBox[{"d", "+", 
      RowBox[{"4", " ", "r"}]}], ")"}]}], "/.", 
   RowBox[{"d", "\[Rule]", 
    RowBox[{"r", 
     SqrtBox[
      RowBox[{"2", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ")"}]}]]}]}]}]}]], "Input",
 CellChangeTimes->{{3.7268714668310013`*^9, 3.726871474678836*^9}, {
  3.726871533697363*^9, 3.726871562088451*^9}, {3.726871697202449*^9, 
  3.726871741691544*^9}}],

Cell["then the fraction overlap is", "Text",
 CellChangeTimes->{{3.726871479981595*^9, 3.726871482284569*^9}, {
  3.726871571815547*^9, 3.7268715797756987`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"fV", "=", 
  RowBox[{
   FractionBox[
    RowBox[{"V", "[", 
     RowBox[{"r", ",", "\[Theta]"}], "]"}], 
    RowBox[{"V", "[", 
     RowBox[{"r", ",", "0"}], "]"}]], "//", "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.726871581301325*^9, 3.7268715899942093`*^9}, {
  3.7268717130470533`*^9, 3.726871714101528*^9}, {3.7268717490091887`*^9, 
  3.726871782873012*^9}}],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "16"], " ", 
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "2"}], "+", 
     SqrtBox[
      RowBox[{"2", "-", 
       RowBox[{"2", " ", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}]]}], ")"}], "2"], " ", 
  RowBox[{"(", 
   RowBox[{"4", "+", 
    SqrtBox[
     RowBox[{"2", "-", 
      RowBox[{"2", " ", 
       RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}]]}], ")"}]}]], "Output",
 CellChangeTimes->{
  3.726871592661663*^9, {3.726871703667808*^9, 3.726871784172249*^9}}]
}, Open  ]],

