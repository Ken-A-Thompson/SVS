Cell["\<\
When n\[LessEqual]2 the integral does not converge with small s and we are \
forced to approximate by integrating up to neutral mutations (-s<1/Ne, where \
Ne is the effective population size)\
\>", "Text",
 CellChangeTimes->{{3.733083781033923*^9, 3.733083817587325*^9}, {
  3.733083867798538*^9, 3.733083922881712*^9}, {3.73308432564229*^9, 
  3.7330843439754353`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   FractionBox[
    RowBox[{"-", "1"}], "s"], 
   RowBox[{"fs", "[", 
    RowBox[{"s", ",", "\[Lambda]", ",", "2"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Integrate", "[", 
   RowBox[{"%", ",", 
    RowBox[{"{", 
     RowBox[{"s", ",", 
      RowBox[{"-", "\[Infinity]"}], ",", 
      RowBox[{
       RowBox[{"-", "1"}], "/", "Ne"}]}], "}"}], ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\[Lambda]", ">", "0"}], ",", 
       RowBox[{"n", "<", "3"}], ",", 
       RowBox[{"Ne", ">", "1"}]}], "}"}]}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Simplify", "[", 
  RowBox[{
   RowBox[{"%%", "/", "%"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"s", "<", "0"}], ",", 
     RowBox[{"\[Lambda]", ">", "0"}], ",", 
     RowBox[{"Ne", ">", "1"}]}], "}"}]}], "]"}]}], "Input",
 CellChangeTimes->{
  3.733082538504793*^9, {3.733083802751522*^9, 3.733083879649642*^9}, 
   3.7330839309772663`*^9, {3.733084136537902*^9, 3.73308415576014*^9}}],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"s", "/", "\[Lambda]"}]], 
   RowBox[{"s", " ", 
    RowBox[{"Gamma", "[", 
     RowBox[{"0", ",", 
      FractionBox["1", 
       RowBox[{"Ne", " ", "\[Lambda]"}]]}], "]"}]}]]}]], "Output",
 CellChangeTimes->{{3.733083805699984*^9, 3.733083861533731*^9}, 
   3.733083905768219*^9, {3.7330841392717037`*^9, 3.733084157773952*^9}}]
}, Open  ]],

