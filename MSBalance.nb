Cell[TextData[{
 "For a continuous-time birth (\[Lambda]) death (\[Mu]) process starting from \
N0 individuals ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"F", "[", 
      RowBox[{"s", ",", "0"}], "]"}], "\[Equal]", 
     SuperscriptBox["s", "N0"]}]}], TraditionalForm]]],
 ") the PGF for the PDF of the number of individuals at time t can be solved \
for explicitly"
}], "Text",
 CellChangeTimes->{{3.682332914730015*^9, 3.6823329437554283`*^9}, {
  3.682413451216722*^9, 3.682413483930725*^9}, {3.682681310190462*^9, 
  3.682681317478837*^9}, {3.7318057368168087`*^9, 3.731805746673211*^9}, {
  3.733081530205077*^9, 3.733081555453239*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"fst", "=", 
  RowBox[{
   RowBox[{"DSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"F", "[", 
           RowBox[{"s", ",", "t"}], "]"}], ",", "t"}], "]"}], "==", 
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"F", "[", 
            RowBox[{"s", ",", "t"}], "]"}], ",", "s"}], "]"}], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"\[Lambda]", " ", 
            SuperscriptBox["s", "2"]}], "-", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"\[Lambda]", "+", "\[Mu]"}], ")"}], "s"}], "+", 
           "\[Mu]"}], ")"}]}]}], ",", 
